{
  "name": "rollup-plugin-shims",
  "version": "2.0.1",
  "description": "Shim only used ES2015+ features in ES5 environment",
  "module": "index.js",
  "scripts": {
    "build:spec": "node build-shim-spec.js > spec.js",
    "test": "mocha -r chai/register-expect -r chai-autoload-plugins -R min '*.spec.js'",
    "lint": "eslint '**.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m7r/rollup-plugin-shims.git"
  },
  "keywords": [
    "rollup",
    "shims",
    "ES2015",
    "ES2016",
    "ES2017",
    "ES2019",
    "ES2020",
    "ES2021"
  ],
  "author": "Martin R. Hufsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m7r/rollup-plugin-shims/issues"
  },
  "homepage": "https://github.com/m7r/rollup-plugin-shims#readme",
  "devDependencies": {
    "acorn": "^8.8.0",
    "chai": "4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-autoload-plugins": "^0.1.0",
    "eslint": "^8.23.1",
    "eslint-plugin-es5": "^1.5.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^17.0.0"
  },
  "dependencies": {
    "acorn-walk": "^8.2.0"
  },
  "peerDependencies": {
    "rollup": ">0.60.0"
  },
  "engines": {
    "node": ">14.0"
  }
}
