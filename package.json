{
  "name": "rollup-plugin-shims",
  "version": "3.0.0",
  "description": "Shim only used ES2015+ features in ES5+ environment",
  "main": "plugin/index.js",
  "type": "module",
  "bin": {
    "build-shim": "./build.js"
  },
  "scripts": {
    "build:test": "npx build-shim --tests --output=test.js",
    "test": "node --test",
    "lint": "eslint && prettier -c ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m7r/rollup-plugin-shims.git"
  },
  "keywords": [
    "rollup",
    "shims",
    "ES2015",
    "ES2016",
    "ES2017",
    "ES2019",
    "ES2020",
    "ES2021",
    "ES2022",
    "ES2023",
    "ES2024"
  ],
  "author": "Martin R. Hufsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m7r/rollup-plugin-shims/issues"
  },
  "homepage": "https://github.com/m7r/rollup-plugin-shims#readme",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "acorn": "^8.12.0",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "prettier": "3.3.2"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "argv-utils": "^1.0.2",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.10"
  },
  "peerDependencies": {
    "rollup": ">2.77.0"
  },
  "engines": {
    "node": ">20.11"
  }
}
